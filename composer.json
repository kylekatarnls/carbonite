{
  "name": "kylekatarnls/carbonite",
  "description": "Freeze, accelerate, slow down time and many more with Carbon.",
  "type": "library",
  "require": {
    "php": "^7.2",
    "nesbot/carbon": "^2.24"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.15.0",
    "phan/phan": "^2.3",
    "phpmd/phpmd": "^2.7",
    "phpstan/phpstan": "^0.11.0",
    "phpunit/phpunit": "^8.3.2",
    "squizlabs/php_codesniffer": "^3.0",
    "vimeo/psalm": "^3.0@dev",
    "povils/phpmnd": "^2.3@dev"
  },
  "autoload": {
    "psr-4": {
      "": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "KyleKatarn",
      "email": "carbon@selfbuild.fr"
    }
  ],
  "minimum-stability": "dev",
  "scripts": {
    "test": [
      "@phpunit",
      "@style-check"
    ],
    "style-check": [
      "@phpcs",
      "@phpcsf",
      "@phpstan",
      "@phpmd",
      "@psalm",
      "@phan"
    ],
    "phan": "phan",
    "phpunit": "phpunit --verbose",
    "phpcs": "phpcs",
    "phpcsf": "php-cs-fixer fix -v --diff --dry-run",
    "phpstan": "phpstan analyse src tests",
    "phpmd": "phpmd src text phpmd.xml",
    "psalm": "psalm"
  }
}
